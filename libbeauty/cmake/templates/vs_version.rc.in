#include <winver.h>

VS_VERSION_INFO         VERSIONINFO
  FILEVERSION           @LIBBEAUTY_VS_VER_FILEVERSION_QUAD@
  PRODUCTVERSION        @LIBBEAUTY_VS_VER_PRODUCTVERSION_QUAD@
  FILEFLAGSMASK         VS_FFI_FILEFLAGSMASK
#ifdef _DEBUG
  FILEFLAGS             1
#else
  FILEFLAGS             0
#endif
  FILEOS                VOS__WINDOWS32
  FILETYPE              VFT_DLL
  FILESUBTYPE           0
BEGIN
  BLOCK "StringFileInfo"
  BEGIN
    BLOCK "040904E4"
    BEGIN
      VALUE "FileDescription",	"@LIBBEAUTY_VS_VER_FILEDESCRIPTION_STR@\0"
      VALUE "FileVersion",	"@LIBBEAUTY_VS_VER_FILEVERSION_STR@\0"
      VALUE "InternalName",	"@LIBBEAUTY_VS_VER_INTERNALNAME_STR@\0"
#if @LIBBEAUTY_VS_VER_HAVE_COPYRIGHT_STR@
      VALUE "LegalCopyright",	"@LIBBEAUTY_VS_VER_COPYRIGHT_STR@\0"
#endif
      VALUE "OriginalFilename",	"@LIBBEAUTY_VS_VER_ORIGINALFILENAME_STR@\0"
      VALUE "ProductName",	"@LIBBEAUTY_VS_VER_PRODUCTNAME_STR@\0"
      VALUE "ProductVersion",	"@LIBBEAUTY_VS_VER_PRODUCTVERSION_STR@\0"
#if @LIBBEAUTY_VS_VER_HAVE_COMMENTS_STR@
      VALUE "Comments",		"@LIBBEAUTY_VS_VER_COMMENTS_STR@\0"
#endif
    END
  END
  BLOCK "VarFileInfo"
  BEGIN
    VALUE "Translation", 0x0409, 1252
  END
END
